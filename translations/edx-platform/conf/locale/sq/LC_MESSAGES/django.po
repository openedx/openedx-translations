#, python-brace-format
msgid "Hint ({hint_num} of {hints_count}): "
msgstr "Këshillë ({hint_num} nga {hints_count}): "

#: xmodule/capa_block.py:1318
#, python-brace-format
msgid ""
"Your answers were previously saved. Click '{button_name}' to grade them."
msgstr ""
"Përgjigjet tuaja janë ruajtur më parë. Kliko '{button_name}' për t'i "
"vlerësuar ato."

#: xmodule/capa_block.py:1394
#, python-brace-format
