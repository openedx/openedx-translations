msgstr "Problemi duhet të rivendoset përpara se të mund të dorëzohet përsëri."

#: xmodule/capa_block.py:1825
#, python-brace-format
msgid "You must wait at least {wait} seconds between submissions."
msgstr "Duhet të prisni të paktën {wait} sekonda ndërmjet paraqitjeve."

#: xmodule/capa_block.py:1835
#, python-brace-format
msgid ""
"You must wait at least {wait_secs} between submissions. {remaining_secs} "
"remaining."
msgstr ""
"Duhet të prisni të paktën {wait_secs} ndërmjet paraqitjeve. {remaining_secs}"
" e mbetur."

#: xmodule/capa_block.py:2011
