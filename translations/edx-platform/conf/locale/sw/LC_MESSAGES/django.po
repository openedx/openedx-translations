msgstr "Ni lazima usubiri angalau sekunde {wait} kati ya mawasilisho."

#: xmodule/capa_block.py:1853
#, python-brace-format
msgid ""
"You must wait at least {wait_secs} between submissions. {remaining_secs} "
"remaining."
msgstr ""
"Ni lazima usubiri angalau {wait_secs} kati ya mawasilisho. {remaining_secs} "
"iliyosalia."

