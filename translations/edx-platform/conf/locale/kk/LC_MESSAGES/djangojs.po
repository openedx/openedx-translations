msgstr "Сіз тіркеліп жатырсыз: {courseName}"

#: lms/templates/verify_student/make_payment_step.underscore:17
#, python-brace-format
msgid "You are upgrading your enrollment for: {courseName}"
msgstr "Сіз келесіге тіркелуді жаңартып жатырсыз: {courseName}"

