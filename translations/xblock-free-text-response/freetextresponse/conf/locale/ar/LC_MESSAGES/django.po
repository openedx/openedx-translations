#, python-brace-format
msgid "Your response must be between {min} and {max} word."
msgid_plural "Your response must be between {min} and {max} words."
msgstr[0] "يجب أن يكون جوابك بين {min} و{max} حرفا."
msgstr[1] "يجب أن يكون جوابك بين {min} و{max} حرفا."
msgstr[2] "يجب أن يكون جوابك بين {min} و{max} حرفا."
msgstr[3] "يجب أن يكون جوابك بين {min} و{max} حروف."
msgstr[4] "يجب أن يكون جوابك بين {min} و{max} حرفا."
msgstr[5] "يجب أن يكون جوابك بين {min} و{max} حرفا."

