#: feedback.py:30
msgid "What did you learn from this? What was missing?"
msgstr "ཁྱེད་ལ་ངེས་ཤེས་ག་རེ་རྙེད་སོང་། མ་ཚང་དེ་འདྲ་འདུག་གམ།"

#: feedback.py:31
msgid "How would you rate this as a learning experience?"
msgstr "ཁྱེད་ཀྱི་ཉམས་མྱོང་ལ་དཔག་ན་འདིར་རིན་ཐང་ཅི་ཙམ་སྤྲོད་ས་རེད།"

#: feedback.py:43 feedback.py:164
msgid "Excellent"
msgstr "ཕུལ་ཏུ་བྱུང་བ།"

#: feedback.py:43 feedback.py:165
msgid "Good"
msgstr "ཡག་པོ།"

#: feedback.py:43 feedback.py:166
msgid "Average"
msgstr "སྤྱིར་བཏང་བ།"

#: feedback.py:43 feedback.py:167
msgid "Fair"
msgstr "འགྲིག་ཙམ།"

#: feedback.py:43 feedback.py:168
msgid "Poor"
msgstr "སྐྱོ་བོ་འདུག"

#: feedback.py:89
msgid "How user voted. -1 if didn't vote"
msgstr "སྤྱོད་མཁན་གྱིས་འོས་ཇི་ལྟར་བླུགས་པ་ -1 གལ་སྲིད་འོས་བླུགས་མེད་ན་"

#: feedback.py:95
msgid "What percent of the time should this show?"
msgstr "འདིར་དུས་ཚོད་ཀྱི་བརྒྱ་ཆ་ག་ཚོད་ཅིག་སྟོན་དགོས་སམ།"

#: feedback.py:105
msgid "A list of user votes"
msgstr "འོས་བསྡུ་བྱས་པའི་ཐོ་གཞུང་ཞིག"

#: feedback.py:109
msgid "Feedback"
msgstr "བསམ་འཆར།"

#: feedback.py:112
msgid "Display Name"
msgstr "འཆར་སྟོན་བྱ་རྒྱུའི་མིང་།"

#: feedback.py:113
msgid "Provide Feedback"
msgstr "བསམ་འཆར་གནང་རོགས།"

#: feedback.py:118 templates/html/studio_view.html:17
msgid "Voting message"
msgstr "འོས་བསྡུ་བྱེད་པའི་འཕྲིན་ཐུང་།"

#: feedback.py:119
msgid "Thank you for voting!"
msgstr "འོས་བླུགས་པར་བཀའ་དྲིན་ཆེ།"

#: feedback.py:124 templates/html/studio_view.html:25
msgid "Feedback message"
msgstr "བསམ་འཆར་གྱི་འཕྲིན་ཐུང་།"

#: feedback.py:125
msgid "Thank you for your feedback!"
msgstr "ཁྱེད་ཀྱི་བསམ་འཆར་ཕུལ་བར་བཀའ་དྲིན་ཆེ་ཞུ།"

#: feedback.py:159
msgid "Please reflect on this course material"
msgstr "སློབ་ཚན་གྱི་རྒྱུ་ཆ་འདིའི་ཐད་དཔྱད་ཞིབ་ཅིག་བྱེད་རོགས།"

#: feedback.py:160
msgid ""
"Please take time to meaningfully reflect on your experience with this course"
" material."
msgstr ""
"ཁྱེད་ཀྱིས་ཆུ་ཚོད་བཏང་སྟེ་སློབ་ཚན་འདི་ལ་སློབ་སྦྱོང་བྱས་པའི་ཉམས་མྱོང་ལ་དཔྱད་ཞིབ་དོན་ལྡན་ཞིག་བྱེད་རོགས།"

#: feedback.py:163
msgid "Please rate your overall experience"
msgstr "ཁྱེད་ཀྱིས་རང་གི་སྤྱི་ཡོངས་ཀྱི་ཉམས་མྱོང་གི་ཚད་ཅིག་སྤྲོད་རོགས།"

#: feedback.py:170
msgid "Please take a moment to thoughtfully reflect."
msgstr ""
"ཁྱེད་ཀྱིས་བསམ་གཞིག་ནན་ཏན་གྱིས་ཆ་ཚོད་ཐུང་ཙམ་བཏང་སྟེ་དཔྱད་ཞིབ་བྱེད་རོགས།"

#: feedback.py:387
msgid "Please vote!"
msgstr "འོས་ཤིག་འཐེན་རོགས།"

#: static/html/feedback_instructor.html:10
msgid "Component"
msgstr "གྲུབ་ཆ།"

#: static/html/feedback_instructor.html:13
msgid "Number of ratings"
msgstr "ཚད་གཞི་སྤྲད་པའི་གྲངས་ཀ"

#: static/html/feedback_instructor.html:16
msgid "Average rating"
msgstr "ཚད་གཞི་སྤྲད་པའི་ཆ་སྙོམས།"

#: static/html/feedback_instructor.html:19
msgid "Feedback "
msgstr "བསམ་འཆར།"

#: static/html/feedback_instructor.html:40
msgid "Comment"
msgstr "མཆན།"

#: static/html/feedback_instructor.html:40
msgid "Vote"
msgstr "འོས་འཐེན།"

#: static/html/feedback_instructor.html:40
msgid "No vote"
msgstr "འོས་མི་འཐེན།"

#: templates/html/feedback.html:17
msgid "Submit Feedback"
msgstr "བསམ་འཆར་འབུལ་སྤྲོད།"

#: templates/html/studio_view.html:9
msgid "Display name"
msgstr "འཆར་སྟོན་བྱ་རྒྱུའི་མིང་།"

#: templates/html/studio_view.html:12
msgid "The name of the component."
msgstr "གྲུབ་ཆ་དེའི་མིང་།"

#: templates/html/studio_view.html:20
msgid "The message to show after user vote."
msgstr "འོས་བླུགས་རྗེས་སྟོན་རྒྱུའི་འཕྲིན་ཐུང་།"

#: templates/html/studio_view.html:28
msgid "The message to show after user feedback."
msgstr "བསམ་འཆར་ཕུལ་རྗེས་སྟོན་རྒྱུའི་འཕྲིན་ཐུང་།"

#: templates/html/studio_view.html:33
msgid "Show aggregate report to students"
msgstr "སྙན་ཐོ་ཕྱོགས་བསྡུས་དེ་སློབ་ཕྲུག་རྣམས་ལ་སྟོན།"

#: templates/html/studio_view.html:35
msgid "False"
msgstr "རྫུན་མ།"

#: templates/html/studio_view.html:36
msgid "True"
msgstr "བདེན་པ།"

#: templates/html/studio_view.html:39
msgid "Whether to show the aggregate report to students."
msgstr "སྙན་ཐོ་ཕྱོགས་བསྡུས་དེ་སློབ་ཕྲུག་རྣམས་ལ་སྟོན་དང་མི་སྟོན།."
