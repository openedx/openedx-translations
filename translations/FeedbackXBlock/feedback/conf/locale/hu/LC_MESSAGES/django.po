msgid "Excellent"
msgstr "Kiváló"

#: feedback.py:43 feedback.py:165
msgid "Good"
msgstr "Jó"

#: feedback.py:43 feedback.py:166
msgid "Average"
msgstr "Átlagos"

#: feedback.py:43 feedback.py:168
msgid "Poor"
msgstr "Gyenge"

