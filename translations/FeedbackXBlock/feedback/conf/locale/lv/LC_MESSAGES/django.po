msgstr "Likerta 4. variants"

#: templates/html/studio_view.html:108
msgid "Example: Fair"
msgstr "Piemērs: apmierinoši"

#: templates/html/studio_view.html:113
msgid "Likert option 5"
msgstr "Likerta 5. variants"

#: templates/html/studio_view.html:116
msgid "Example: Poor"
msgstr "Piemērs: slikti"

