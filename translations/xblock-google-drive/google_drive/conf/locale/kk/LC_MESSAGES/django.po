#: templates/html/google_docs_edit.html:47
msgid "Save"
msgstr "Сақтау"

#: templates/html/google_calendar_edit.html:64
#: templates/html/google_docs_edit.html:51
msgid "Cancel"
msgstr "Болдырмау"

#: templates/html/google_docs_edit.html:9
