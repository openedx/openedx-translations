#: models.py:25
msgid "Display Name"
msgstr "འཆར་སྟོན་བྱ་རྒྱུའི་མིང་།"

#: models.py:28
msgid "This is the XBlock's display name"
msgstr "འདི་ནི་ XBlock ཡི་འཆར་མིང་ཡིན།"

#: models.py:32
msgid "Image URL"
msgstr "འདྲ་པར་གྱི་དྲ་ཐག"

#: models.py:41
msgid "This is the location of the full-screen image to be displayed."
msgstr "འདི་ནི་འདྲ་པར་དེ་འཆར་ངོས་ཆ་ཚང་གང་བར་སྟོན་རྒྱུའི་ས་གནས་ཡིན།"

#: models.py:46
msgid "Thumbnail URL"
msgstr "མཚོན་བྱེད་འདྲ་པར་གྱི་དྲ་ཐག"

#: models.py:50
msgid ""
"This is the (optional) location of a thumbnail image to be displayed before "
"the main image has been enlarged."
msgstr ""
"འདི་ནི་མ་ཕྱིའི་འདྲ་པར་ཆེ་རུ་མ་བཏང་གོང་དུ་འཆར་རྒྱུའི་མཚོན་བྱེད་འདྲ་པར་ "
"(གདམ་ཀ) གྱི་གནས་ཡིན།"

#: models.py:56
msgid "Description"
msgstr "འགྲེལ་བཤད།"

#: models.py:59
msgid "Description text, displayed to screen readers"
msgstr "འགྲེལ་བཤད། འཆར་ངོས་སུ་ལྟ་མཁན་རྣམས་ལ་སྟོན་ཡོང་།"

#: models.py:64
msgid "Alt Text"
msgstr "Alt ཡིག་འབྲུ།"

#: models.py:68
msgid ""
"This field allows you to add alternate or descriptive text that pertains to "
"your image."
msgstr ""
"ཚན་པ་འདིས་ཁྱེད་ལ་རང་ཉིད་ཀྱི་འདྲ་པར་དང་འབྲེལ་ཐོགས་པའི་འདྲ་མཚུངས་སམ་ཡང་ན་འགྲེལ་བརྗོད་རྣམས་སྣོན་པའི་ཆོག་མཆན་སྤྲོད་པ་ཡིན།"

#: templates/view.html:15
msgid "Fullscreen"
msgstr "འཆར་ངོས་ཧྲིལ་བོ།"

#: templates/view.html:25
msgid "Zoom In"
msgstr "རྒྱ་བསྐྱེད།"

#: templates/view.html:29
msgid "Close"
msgstr "སྒོ་རྒྱོབས།"
